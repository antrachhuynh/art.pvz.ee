{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      },
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "handle": "filesystem",
        "src": "api/.*"
      },
      {
        "handle": "filesystem",
        "src": "static/.*"
      },
      {
        "handle": "filesystem",
        "src": "favicon.ico"
      },
      {
        "handle": "next",
        "src": ".*"
      }
    ]
  }
  